{
    "usesTypeScript": true,
    "usesStyledComponents": false,
    "usesCssModule": true,
    "cssPreprocessor": "css",
    "testLibrary": "Testing Library",
    "component": {
        "default": {
            "path": "src/components",
            "withStyle": true,
            "withTest": true,
            "withStory": true,
            "withLazy": true
        },
        "uicmp": {
            "path": "src/components/ui",
            "withStyle": true,
            "withTest": true,
            "withStory": true,
            "withLazy": true,
            "customTemplates": {
                "component": "grc_template/uiTemplate/TemplateName.js"
            }
        },
        "layoutcmp": {
            "path": "src/components/layout",
            "withStyle": true,
            "withTest": true,
            "withStory": true,
            "withLazy": true,
            "customTemplates": {
                "component": "grc_template/layouttemplate/TemplateName.tsx",
                "test": "grc_template/layouttemplate/TemplateName.stories.tsx",
                "story": "grc_template/layouttemplate/TemplateName.test.tsx"
            }
        }
    }
}
